{
  "name": "graph-test",
  "description": "A test runtime for subgraph modules.",
  "version": "0.0.1",
  "author": "Gnosis developers <developers@gnosis.io>",
  "license": "(MIT OR Apache-2.0)",
  "repository": "https://github.com/gnosis/graph-test.git",
  "homepage": "https://github.com/gnosis/graph-test",
  "bugs": "https://github.com/gnosis/graph-test/issues",
  "keywords": [
    "ethereum",
    "graph",
    "protocol",
    "testing",
    "wasm"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --max-warnings 0 .",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "test:e2e": "yarn test:e2e:build && yarn test:e2e:run",
    "test:e2e:build": "./e2e/subgraph/build.sh",
    "test:e2e:run": "mocha -r ts-node/register e2e/**/*.test.ts"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.18.1",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "chai": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "solc": "^0.7.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
